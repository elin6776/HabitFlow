{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "paths": {
      "@/*": ["./*"]
    },
    "allowJs": true,
    "jsx": "react-native"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.jsx",
    "app/index.jsx",
    "app/(tabs)/challenges.js",
    "expo-env.d.ts"
  ]
}
